{
    "compilerOptions": {
      "target": "ES6", // Или более новая версия
      "module": "NodeNext",
      "moduleResolution": "nodenext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "" // Папка для скомпилированного кода
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }